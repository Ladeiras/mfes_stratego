class Rank

	instance variables
		public name : seq of char;
		public avaiability : nat1;
		public movement : nat;
		public number : nat1;
	
	operations
		public Rank : (seq of char)*nat1*nat*nat1 ==> Rank
			Rank(n,a,m,nr) ==
			( 
				name := n;
				avaiability := a;
				movement := m;
			)
			post name = n and avaiability = a and movement = m;
			
end Rank