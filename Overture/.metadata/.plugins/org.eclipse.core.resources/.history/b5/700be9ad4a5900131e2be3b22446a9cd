class Rank

	instance variables
		public name : seq of char;
		public avaiability : nat1;
		public movement : nat;
		public number : nat1;
	
	operations
		public Rank : seq of char ==> Rank
			Rank(n) ==
			( 
				name := n;
				avaiability := a;
				movement := m;
				number := nr;
			)
			post name = n and 
					 avaiability = a and 
					 movement = m and
					 number = nr;
			
end Rank